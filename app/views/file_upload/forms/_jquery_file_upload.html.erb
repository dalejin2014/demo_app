<% content_for :javascript do %>
  <%= javascript_include_tag "file_upload/#{file_upload_form.upload_type}" %>

<% end %>

<%= simple_form_for(file_upload_form, url: file_upload_path,
                    html: { id: "#{file_upload_form.upload_type}-id", multipart: true}  ) do |f| %>
  <%= f.hidden_field :upload_type %>
  <%= f.file_field :file, accept: 'text/csv' %>
  <%= f.button :submit, "submit" %>
<% end %>